            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">// Путь к файлу для сохранения данных
def filePath = &apos;results.txt&apos;

// Получаем название и страну происхождения выбранной породы
def breedName = vars.get(&quot;breedName&quot;)
def breedOrigin = vars.get(&quot;breedOrigin&quot;)

// Открываем файл для записи в режиме добавления
def file = new File(filePath)
file.append(&quot;Breed: ${breedName}, Origin: ${breedOrigin}\n&quot;)

log.info(&quot;Data saved to file: ${filePath}&quot;)</stringProp>
          </JSR223PostProcessor>
