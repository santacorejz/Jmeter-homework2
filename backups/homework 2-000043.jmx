          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Select Random Cat Breed">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import groovy.json.JsonSlurper

// Получаем список пород кошек из предыдущего запроса
def breedsJson = prev.getResponseDataAsString()
def breeds = new JsonSlurper().parseText(breedsJson).data

// Выбираем рандомную породу кошки
def randomBreed = breeds[new Random().nextInt(breeds.size())]

// Сохраняем название и страну происхождения выбранной породы
vars.put(&quot;breedName&quot;, randomBreed.breed)
vars.put(&quot;breedOrigin&quot;, randomBreed.origin)

log.info(&quot;Selected breed: &quot; + vars.get(&quot;breedName&quot;))
log.info(&quot;Origin: &quot; + vars.get(&quot;breedOrigin&quot;))</stringProp>
          </JSR223PostProcessor>
